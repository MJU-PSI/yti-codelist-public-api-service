{
  "index": {
    "max_result_window": 10000,
    "mapping.total_fields.limit": 5000,
    "number_of_shards": 1
  },
  "analysis": {
    "analyzer": {
      "text_analyzer": {
        "type": "custom",
        "tokenizer": "keyword",
        "filter": [
          "lowercase",
          "trim"
        ]
      }
    },
    "normalizer": {
      "keyword_normalizer": {
        "type": "custom",
        "filter": [
          "lowercase"
        ]
      }
    }
  }
}
